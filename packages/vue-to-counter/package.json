{
  "name": "vue-to-counter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/types/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run type-gen",
    "type-check": "vue-tsc -b",
    "type-gen": "vue-tsc --declaration --emitDeclarationOnly --declarationDir dist/types -p tsconfig.app.json --noEmit false",
    "preview": "vite preview",
    "prepare": "husky install",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "files": [
    "dist",
    "typings/global.d.ts"
  ],
  "dependencies": {
    "@types/node": "^22.9.3",
    "@vueuse/core": "^11.3.0",
    "d3-array": "^3.2.4",
    "d3-interpolate": "^3.0.1",
    "date-fns": "^4.1.0",
    "decimal.js-light": "^2.5.1",
    "lodash-es": "^4.17.21",
    "uuid": "^11.0.3",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@types/d3-array": "^3.2.1",
    "@types/d3-interpolate": "^3.0.4",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "3.3.3",
    "sass-embedded": "^1.81.0",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vitepress": "^1.5.0",
    "vue-tsc": "^2.1.8"
  }
}
